<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <Description>Free Train Simulator Updater</Description>
    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyTitle>Free Train Simulator Updater</AssemblyTitle>
    <GenerateResourceUsePreserializedResources>true</GenerateResourceUsePreserializedResources>
    <OutputPath>..\..\Program</OutputPath>
    <RootNamespace>FreeTrainSimulator.Updater</RootNamespace>
    <ApplicationIcon>..\FTS.ico</ApplicationIcon>
    <Configurations>Debug;Release;CodeAnalysis</Configurations>
		<ForceDesignerDpiUnaware>true</ForceDesignerDpiUnaware>
	</PropertyGroup>
  <ItemGroup>
    <PackageReference Include="GetText.NET" Version="8.0.5" />
    <PackageReference Include="GetText.NET.WindowsForms" Version="8.0.5" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="System.Resources.Extensions" Version="9.0.2" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\FreeTrainSimulator.Updater\FreeTrainSimulator.Updater.csproj" />
  </ItemGroup>
</Project>