<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <UseWindowsForms>true</UseWindowsForms>
    <DisableWinExeOutputInference>true</DisableWinExeOutputInference>
    <Description>Open Rails Content Checker (Contributed)</Description>
    <AssemblyTitle>Open Rails Content Checker (Contributed)</AssemblyTitle>
    <OutputPath>..\..\..\Program</OutputPath>
    <RootNamespace>Orts.ContentChecker</RootNamespace>
    <AssemblyName>Contrib.ContentChecker</AssemblyName>
    <ApplicationIcon>..\..\Orts.ico</ApplicationIcon>
    <Configurations>Debug;Release;CodeAnalysis</Configurations>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="MonoGame.Framework.WindowsDX" Version="3.8.3" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\ActivityRunner\ActivityRunner.csproj" />
    <ProjectReference Include="..\..\Orts.Simulation\Orts.Simulation.csproj" />
  </ItemGroup>
</Project>